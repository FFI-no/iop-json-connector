  "properties": {
    "HeaderRec": {
      "type": "object",
      "comment": "",
      "required": [
        "MessageID"
      ],
      "properties": {
        "MessageID": {
          "type": "string",
          "jausType": "unsigned short integer",
          "comment": "A two byte field to hold the message ID of a message",
          "const": "4b02"
        }
      }
    },
    "SubsystemList": {
      "type": "array",
      "comment": "",
      "required": [
        "SubsystemRec"
      ],
      "items": {
        "anyOf": [
          {
            "type": "object",
            "comment": "",
            "required": [
              "SubsystemID",
              "NodeID",
              "ComponentID"
            ],
            "properties": {
              "SubsystemID": {
                "type": "number",
                "jausType": "unsigned short integer",
                "comment": "Subsystem ID"
              },
              "NodeID": {
                "type": "number",
                "jausType": "unsigned byte",
                "comment": "Node ID used for Query Configuration."
              },
              "ComponentID": {
                "type": "number",
                "jausType": "unsigned byte",
                "comment": "Component ID used for Query Configuration."
              }
            }
          }
        ]
      },
      "jausType": "unsigned byte",
      "minItems": null,
      "maxItems": null,
      "isVariant": false
    }
  },
  "required": [
    "HeaderRec",
    "SubsystemList"
  ]
