{
  "name": "ReportUGVSummary",
  "messageId": "fd10",
  "isCommand": false,
  "description": "ReportUGVSummary provides a simple summary of the health status of the UGV to any requestor. The summary contains a list of (NodeID, severityCode) pairs, one per CM, in which the highest active severityCode is reported for each CM. This list is preceded in the message by a single severityCode for the UGV, which is the highest severity code reported by any CM in the UGV. This allows a HealthMonitor client to identify the CMs responsible for any UGV faults, and to subsequently query the HealthReporter of the responsible CM or (CMs) for full details.",
  "type": "object",
  "properties": {
    "HeaderRec": {
      "type": "object",
      "comment": "",
      "required": [
        "MessageID"
      ],
      "properties": {
        "MessageID": {
          "type": "string",
          "jausType": "unsigned short integer",
          "comment": "",
          "const": "fd10"
        }
      }
    },
    "UGVHealthSummarySequence": {
      "type": "object",
      "comment": "",
      "required": [
        "UGVSeverityRec",
        "SeverityList"
      ],
      "properties": {
        "UGVSeverityRec": {
          "type": "object",
          "comment": "",
          "required": [
            "severity"
          ],
          "properties": {
            "severity": {
              "type": "string",
              "jausType": "unsigned byte",
              "comment": "Enumeration of severity codes, as defined below:",
              "valueSet": [
                {
                  "valueEnum": {
                    "enumIndex": 0,
                    "enumConst": "NONE"
                  }
                },
                {
                  "valueEnum": {
                    "enumIndex": 1,
                    "enumConst": "INFO"
                  }
                },
                {
                  "valueEnum": {
                    "enumIndex": 2,
                    "enumConst": "WARN"
                  }
                },
                {
                  "valueEnum": {
                    "enumIndex": 3,
                    "enumConst": "ERR"
                  }
                },
                {
                  "valueEnum": {
                    "enumIndex": 4,
                    "enumConst": "FAIL"
                  }
                }
              ],
              "enum": [
                "NONE",
                "INFO",
                "WARN",
                "ERR",
                "FAIL"
              ]
            }
          }
        },
        "SeverityList": {
          "type": "array",
          "comment": "List of Node Severity Levels",
          "required": [
            "SeveritySequence"
          ],
          "items": {
            "anyOf": [
              {
                "type": "object",
                "comment": "",
                "required": [
                  "NodeID",
                  "SeverityRec"
                ],
                "properties": {
                  "NodeID": {
                    "type": "object",
                    "comment": "",
                    "required": [
                      "NodeID"
                    ],
                    "properties": {
                      "NodeID": {
                        "type": "number",
                        "jausType": "unsigned byte",
                        "comment": "JAUS Node ID"
                      }
                    }
                  },
                  "SeverityRec": {
                    "type": "object",
                    "comment": "",
                    "required": [
                      "severity"
                    ],
                    "properties": {
                      "severity": {
                        "type": "string",
                        "jausType": "unsigned byte",
                        "comment": "Enumeration of severity codes, as defined below:",
                        "valueSet": [
                          {
                            "valueEnum": {
                              "enumIndex": 0,
                              "enumConst": "NONE"
                            }
                          },
                          {
                            "valueEnum": {
                              "enumIndex": 1,
                              "enumConst": "INFO"
                            }
                          },
                          {
                            "valueEnum": {
                              "enumIndex": 2,
                              "enumConst": "WARN"
                            }
                          },
                          {
                            "valueEnum": {
                              "enumIndex": 3,
                              "enumConst": "ERR"
                            }
                          },
                          {
                            "valueEnum": {
                              "enumIndex": 4,
                              "enumConst": "FAIL"
                            }
                          }
                        ],
                        "enum": [
                          "NONE",
                          "INFO",
                          "WARN",
                          "ERR",
                          "FAIL"
                        ]
                      }
                    }
                  }
                }
              }
            ]
          },
          "jausType": "unsigned byte",
          "minItems": 0,
          "maxItems": 255,
          "isVariant": false
        }
      }
    }
  },
  "required": [
    "HeaderRec",
    "UGVHealthSummarySequence"
  ]
}